<?xml version="1.0" encoding="UTF-8"?>
<!-- 
    Demonstrates the use of Foxvalue constraints: declares a FoxvalueEqeq contraint.
    
    Each value item must be found in the term list contained by <eqeq>, and each item found 
    there must be equal to a value item. Note that this does not mean "deep equal", as neither 
    the order nor the number of occurrences are checked. The value may, for example, contain 
    for each term from the term list any number of repetitions.
-->
<greenfox greenfoxURI="http://www.greenfox.org/ns/schema-examples/demo-constraint-component/foxvalue.eqeq"
          xmlns="http://www.greenfox.org/ns/schema">

    <context>
        <field name="domain" value="${schemaPath}\..\..\..\..\example-data"/>
    </context>

    <domain path="${domain}" name="greenfox-example-data">
        <folder foxpath="airports\ireland" id="airportsFolderShape">
            <targetSize countMsg="Missing folder: airports/ireland" count="1"/>
            <foxvalues>
                <foxvalue exprFOX=".\\airport-*.xml//city"
                          minCount="1" minCountMsg="At least one city value expected."
                          eqeq="City list not perfect.">
                    <eqeq>
                        <term>Bantry</term>
                        <term>Dongloe</term>
                        <term>Dublin</term>
                        <term>Galway</term>
                        <term>Inishmaan</term>
                        <term>Inisheer</term>
                        <term>Inis Mor</term>
                        <term>Kerry</term>
                        <term>Indreabhan</term>
                        <term>Connaught</term>
                        <term>Cork</term>
                        <term>Shannon</term>
                        <term>Sligo</term>
                        <term>Waterford</term>
                    </eqeq>
                </foxvalue>
            </foxvalues>            
        </folder>
    </domain>

</greenfox>
